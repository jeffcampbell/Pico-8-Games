pico-8 cartridge // http://www.pico-8.com
version 7
__lua__
function _init()
 add(doge,dogeconstruct(40,64))
end

doge={}
dogeconstruct=function(x,y)
 local obj={}
 obj.pos={x=x,y=y}
 obj.hitbox={x=2,y=1,w=5,h=6}
 obj.state=0
 obj.dir=0
 obj.pat=0
 
 obj.update=function(o)
  o.pos.x=(o.pos.x+128)%128 --no bounds left and right
  o.pat+=1 --increment state clock
 end
 
 obj.draw=function(o)
  --idle anim
  if(o.state==0)sspr(0,0,16,16,o.pos.x,o.pos.y)
  --walk anim left
  if(o.state==1 and o.dir==-1)anim(o,0,16,3,true)
  --walk anim right
  if(o.state==1 and o.dir==1)anim(o,0,16,3)
  --fall anim
  if(o.state==2)anim(o,0,0,3)
  --jump anim
  if(o.state==3)anim(o,0,0,3)
 end

 obj.idle=function(o)
  if o.state==0 then 
   if(btn(0) or btn(1))change_state(o,1)
   if(btn(2))change_state(o,3)
   if(canfall(o))change_state(o,2)
  end
 end
 
 obj.walk=function(o)
  if o.state==1 then
   if(btn(0))o.dir=-1
   if(btn(1))o.dir=1
   o.pos.x+=o.dir*min(o.pat,2)  
   if(not(btn(0) or btn(1)))change_state(o,0)
   if(btn(2))change_state(o,3)
   if(canfall(o))change_state(o,2)
  end
 end
 
 obj.fall=function(o)
  if o.state==2 then
   if(canfall(o))then
    if(btn(0))o.pos.x-=1 --steer left
    if(btn(1))o.pos.x+=1 --steer right
    o.pos.y+=min(4,o.pat) --move the player
    --check ground contact
    if(not canfall(o))o.pos.y=flr(o.pos.y/8)*8
   else
    o.pos.y=flr(o.pos.y/8)*8 --fix position when we hit the ground
    change_state(o,0)
   end
  end
 end

 obj.jump=function(o)
  if o.state==3 then
   o.pos.y-=6-o.pat
   if(btn(0))o.pos.x-=2
   if(btn(1))o.pos.x+=2
   if(not btn(2) or o.pat>7)change_state(o,0)
  end
 end

 return obj
end

function anim(o,sx,sy,nf,fl)
local sw=16
local sh=16
local sp=10
	if(not o.a_ct) o.a_ct=0
	if(not o.a_st)	o.a_st=0

	o.a_ct+=1

	if(o.a_ct%(30/sp)==0) then
	 o.a_st+=16
	 if(o.a_st==nf*sw) o.a_st=0
	end

	o.a_fr=sx+o.a_st

sspr(o.a_fr,sy,sw,sh,o.pos.x,o.pos.y,sw,sh,fl)
end

function collide(obj,other)
 if
  other.pos.x+other.hitbox.x+other.hitbox.w > obj.pos.x+obj.hitbox.x and 
  other.pos.y+other.hitbox.y+other.hitbox.h > obj.pos.y+obj.hitbox.y and
  other.pos.x+other.hitbox.x < obj.pos.x+obj.hitbox.x+obj.hitbox.w and
  other.pos.y+other.hitbox.y < obj.pos.y+obj.hitbox.y+obj.hitbox.h 
 then
  return true
 end
end

function canfall(obj)
 --get the map tile under the player
 v=mget(flr((obj.pos.x+4)/8),flr((obj.pos.y+16)/8))
 --see if it's flagged as wall
 return not fget(v,0)
end

function change_state(obj,s)
 obj.state=s
 obj.pat=0
end

doge.update=function()
	foreach(doge, function(obj)
  obj.update(obj)
  obj.idle(obj)
  obj.walk(obj)
  obj.fall(obj)
  obj.jump(obj)
 end)
end

doge.draw=function()
	foreach(doge, function(obj)
  obj.draw(obj)
 end)
end

function _update() 
doge.update()
end

function _draw()
 cls()
 map(0,0,0,0,16,16)
 doge.draw()
 print(doge[1]["state"],2,2,2)
end

