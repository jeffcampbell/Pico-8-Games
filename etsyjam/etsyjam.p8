pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
score=100
makeenemies=1
startscreen=1
add(ships,shipconstruct(64,110))
end

function shipconstruct(x,y)
local o={}
o.pos={x=x,y=y}
o.speed=2
o.hitbox={x=0,y=0,w=8,h=8}

o.update=function(o)
 if (btn(0)) then
 o.pos.x-=o.speed
 elseif (btn(1)) then
 o.pos.x+=o.speed
-- elseif (btn(2)) then
-- o.pos.y-=o.speed
-- elseif (btn(3)) then
-- o.pos.y+=o.speed
 end
end

o.shoot=function(o)
 if (btnp(2)) then
 add(bullets,bulletconstruct(o.pos.x,o.pos.y))
	score-=1
	sfx(00)
 end 
end

o.draw=function(o)
	spr(00,o.pos.x,o.pos.y)
end

return o
end

function bulletconstruct(x,y)
local o={}
o.pos={x=x,y=y}
o.speed=3
o.hitbox={x=3,y=3,w=3,h=3}


o.update=function(o)
 if (o.pos.y>0) then
 o.pos.y-=o.speed
 else
 del(bullets,o)
 end
end

o.draw=function(o)
	spr(016,o.pos.x,o.pos.y)
end

o.explode=function(bullet,enemy)
  for enemy in all(enemies) do
   if collide(bullet,enemy) then
--    sfx(00)
    score+=10
    sfx(01)
--    add(explosions,explosionconstruct(zombie.pos.x,zombie.pos.y))
    add(explosions,explosionconstruct(enemy.pos.x,enemy.pos.y))
    del(enemies,enemy)
    del(bullets,bullet)
   end
  end
end

return o
end

function enemyconstruct(x,y,l)
local o={}
o.pos={x=x,y=y}
o.speed=1
o.letter=l
o.hitbox={x=2,y=2,w=4,h=4}


o.update=function(o)
 if (o.pos.y<128) then
 o.pos.y+=o.speed
 else
 del(enemies,o)
 end
end

o.draw=function(o)
	print(o.letter,o.pos.x,o.pos.y,9)
end

o.explode=function(enemy,ship)
  for ship in all(ships) do
   if collide(enemy,ship) then
   del(ships,ship)
   sfx(02)
   end
  end
end

return o
end

function explosionconstruct(x,y)
local o={}
o.pos={x=x,y=y}
o.timer=10

o.update=function(explosion)
if (explosion.timer>0) then
explosion.timer-=1
else
del(explosions,explosion)
end
end

o.draw=function(o)
	spr(002,o.pos.x,o.pos.y)
end

return o
end

function collide(obj,other)
    if
     other.pos.x+other.hitbox.x+other.hitbox.w > obj.pos.x+obj.hitbox.x and 
     other.pos.y+other.hitbox.y+other.hitbox.h > obj.pos.y+obj.hitbox.y and
     other.pos.x+other.hitbox.x < obj.pos.x+obj.hitbox.x+obj.hitbox.w and
     other.pos.y+other.hitbox.y < obj.pos.y+obj.hitbox.y+obj.hitbox.h 
    then
        return true
    end
end

create_enemy=function()
local x=flr(rnd(100))+5

local chance=flr(rnd(20))
if (chance==0) then
local dice=flr(rnd(4))
if (dice==0) then
add(enemies,enemyconstruct(x,0,"e"))
elseif (dice==1) then
add(enemies,enemyconstruct(x,0,"t"))
elseif (dice==2) then
add(enemies,enemyconstruct(x,0,"s"))
elseif (dice==3) then
add(enemies,enemyconstruct(x,0,"y"))
end
end
end

ships={}
ship_update=function()
	foreach(ships, function(o)
	o.update(o)
	o.shoot(o)
end)
end

explosions={}
explosion_update=function()
	foreach(explosions, function(o)
	o.update(o)
end)
end

enemies={}
enemy_update=function()
	foreach(enemies, function(o)
	o.update(o)
	o.explode(o)
end)
end

bullets={}
bullet_update=function()
	foreach(bullets, function(o)
	o.update(o)
	o.explode(o)
end)
end

ship_draw=function()
	foreach(ships, function(o)
	o.draw(o)
end)
end

explosion_draw=function()
	foreach(explosions, function(o)
	o.draw(o)
end)
end

enemy_draw=function()
	foreach(enemies, function(o)
	o.draw(o)
end)
end

bullet_draw=function()
	foreach(bullets, function(o)
	o.draw(o)
end)
end

function _update()
if (makeenemies==1) then
create_enemy()
end
ship_update()
bullet_update()
enemy_update()
explosion_update()
if (#ships==0) then
if (btnp(4)) then
score=100
add(ships,shipconstruct(64,110))
end
end

end

function _draw()
cls()
ship_draw()
bullet_draw()
explosion_draw()
enemy_draw()
print(score,2,2,2)
if (#ships==0) then
print("game over",50,64,8)
end
end
