pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
add(scopes,scopeconstruct(64,64))
end

scopeconstruct=function(x,y)
local o={}
o.pos={x=x,y=y}
o.state=0
o.speed=3
o.control=function(o)
 if (btnp(1)) then
  o.pos.x+=o.speed
 end 
 if (btnp(2)) then
  o.pos.y-=o.speed
 end 
 if (btnp(3)) then
  o.pos.y+=o.speed
 end 
 if (btnp(0)) then
  o.pos.x-=o.speed
 end
end

o.examine=function(scope,star)
 for star in all(stars) do
  if look(scope,star) then
  -- scope.state=1
   star.state=1
  else
  -- scope.state=0
   star.state=0
  end
 end
end

o.draw=function(o)
 if (o.state==0) then
  sspr(0,0,16,16,o.pos.x,o.pos.y)
  print(o.state,2,2,4)
 elseif (o.state==1) then
  sspr(16,0,32,16,o.pos.x,o.pos.y)
  print(o.state,2,2,4)
 end 
end

return o
end

starconstruct=function(x,y,star,h,nh)
local o={}

--star position
o.pos={x=x,y=y}

--number of planets
o.planets={habitable=h,nonhabitable=nh}

--star designation
o.star=star

--state machine
o.state=0

o.draw=function(o)
  spr(48,o.pos.x,o.pos.y)
end

o.drawname=function(o)
 if (o.state==1) then
  local name=(o.star .. "s" .. o.planets.habitable .. "e" .. o.planets.nonhabitable .. "g")
  print(name,o.pos.x-8,o.pos.y+16,9)
 else
 end
end

return o
end

function look(obj,other)
 if
  other.pos.x+3 > obj.pos.x+4 and 
  other.pos.y+3 > obj.pos.y+4 and
  other.pos.x+3 < obj.pos.x+11 and
  other.pos.y+3 < obj.pos.y+11 
 then
  return true
 end
end

--create scope table
--and manage scope actions
scopes={}

scopes.update=function()
 foreach(scopes, function(o)
  o.control(o)
  o.examine(o)
 end)
end

scopes.draw=function()
 foreach(scopes, function(o)
  o.draw(o)
 end)
end

--create stars table
--and manage star actions
stars={}
stars.draw=function()
 foreach(stars, function(o)
  o.draw(o)
  o.drawname(o)
 end)
end

starcount=0
stars.create=function()
 if (starcount<100) then
  local x=flr(rnd(128))
  local y=flr(rnd(128))
  local habitable=flr(rnd(3))
  local nonhabitable=flr(rnd(10))
  add(stars,starconstruct(x,y,#stars,habitable,nonhabitable))
  starcount+=1
 end
end

function _update()
 stars.create()
 scopes.update()
end

function _draw()
 cls()
 stars.draw()
 scopes.draw()
end
